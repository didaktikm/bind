$TTL 3600
$ORIGIN dnsweb.lab.
@               IN      SOA     ns01.dns.lab. root.dnsweb.lab. (
                            2019081601 ; serial
                            3600       ; refresh (1 hour)
                            600        ; retry (10 minutes)
                            86400      ; expire (1 day)
                            600        ; minimum (10 minutes)
                        )

                IN      NS      ns01.dnsweb.lab.
                IN      NS      ns02.dnsweb.lab.

; DNS Servers
ns01            IN      A       192.168.50.10
ns02            IN      A       192.168.50.11
; Clients
client         IN      A       192.168.50.15
web1           IN      CNAME   client
